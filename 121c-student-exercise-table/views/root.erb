<table border="1">
  <% @students.each do |student| %>
    <tr>
      <td>
        <strong> <%= student.first_name %> <%= student.last_name %>
      </td>
        <% @exercises.each do |exercise| %>
          <td>
            <%= exercise.directory %>
          </td>
        <% end %>
      </tr>
  <% end %>
</table>